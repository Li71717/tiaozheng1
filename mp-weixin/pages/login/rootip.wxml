<view>
  <u-navbar
    vue-id="7985b19e-1"
    bgColor="#62CDF7"
    title="设置服务端地址"
    border="{{true}}"
    placeholder="{{true}}"
    autoBack="{{true}}"
    data-event-opts="{{[['^leftClick',[['goBack']]]]}}"
    bind:leftClick="__e"
    bind:__l="__l"
  ></u-navbar>
  <scroll-view class="svcontainer" enable-flex="{{true}}">
    <u--form
      class="vue-ref"
      vue-id="7985b19e-2"
      labelPosition="left"
      model="{{fobj}}"
      data-ref="regform"
      bind:__l="__l"
      vue-slots="{{['default']}}"
    >
      <u-form-item
        vue-id="{{('7985b19e-3')+','+('7985b19e-2')}}"
        borderBottom="{{true}}"
        label-width="76"
        label="根域名:"
        prop="ip"
        bind:__l="__l"
        vue-slots="{{['default']}}"
      >
        <u--input
          bind:input="__e"
          vue-id="{{('7985b19e-4')+','+('7985b19e-3')}}"
          border="none"
          value="{{fobj.ip}}"
          data-event-opts="{{[['^input',[['__set_model',['$0','ip','$event',[]],['fobj']]]]]}}"
          bind:__l="__l"
        ></u--input>
      </u-form-item>
      <u-form-item
        vue-id="{{('7985b19e-5')+','+('7985b19e-2')}}"
        borderBottom="{{true}}"
        label-width="76"
        label="端口:"
        prop="duankou"
        bind:__l="__l"
        vue-slots="{{['default']}}"
      >
        <u--input
          bind:input="__e"
          vue-id="{{('7985b19e-6')+','+('7985b19e-5')}}"
          border="none"
          type="number"
          value="{{fobj.duankou}}"
          data-event-opts="{{[['^input',[['__set_model',['$0','duankou','$event',[]],['fobj']]]]]}}"
          bind:__l="__l"
        ></u--input>
      </u-form-item>
    </u--form>
    <view class="hpaddingctn">
      <u-button
        vue-id="7985b19e-7"
        type="primary"
        text="提交"
        data-event-opts="{{[['^click',[['save']]]]}}"
        bind:click="__e"
        bind:__l="__l"
      ></u-button>
    </view>
  </scroll-view>
</view>