<view>
  <u-form-item
    vue-id="fa89224c-1"
    borderBottom="{{true}}"
    label-width="{{lw}}"
    label="{{label}}"
    data-event-opts="{{[['^click',[['e0']]]]}}"
    bind:click="__e"
    bind:__l="__l"
    vue-slots="{{['default','right']}}"
  >
    <u--input
      bind:input="__e"
      vue-id="{{('fa89224c-2')+','+('fa89224c-1')}}"
      border="none"
      type="select"
      value="{{sitem}}"
      data-event-opts="{{[['^input',[['__set_model',['','sitem','$event',[]]]]]]}}"
      bind:__l="__l"
    ></u--input>
    <u-icon
      vue-id="{{('fa89224c-3')+','+('fa89224c-1')}}"
      slot="right"
      name="arrow-right"
      bind:__l="__l"
    ></u-icon>
    <u-picker
      class="vue-ref"
      vue-id="{{('fa89224c-4')+','+('fa89224c-1')}}"
      show="{{ssshow3}}"
      columns="{{datalist}}"
      data-ref="uPicker"
      data-event-opts="{{[['^cancel',[['e1']]],['^confirm',[['confirmDate']]]]}}"
      bind:cancel="__e"
      bind:confirm="__e"
      bind:__l="__l"
    ></u-picker>
  </u-form-item>
</view>